     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>ZKBETR_KOND</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
